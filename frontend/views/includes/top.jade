docktype
meta(charset="utf-8") 
html(lang="en")
	head
		title Catalog
		link(rel="stylesheet", href="/css/style.css")
		script(src="/lib/require.js", data-main="/app/boot")
	body
		nav: ul
			li: a#link1 
				| link1
			li: a#link2 
				| link2
			if(logged)
				li: a(href="/users/#{logged}")
					| #{logged}
				li: a#logout(href="#")
					|logout
		div#content
			| 